write file "selenolate-proteinprep_1-impref-out_ref0.out" -
  title "selenolate-proteinprep_1-impref-out minimization, level 0" *


SET FFIELD OPLS_2005

CREATE
build primary name complex type auto read maestro file -
     "/home/nayanika/github/PhD_Thesis/EVB/EVB-Q6/openMM-OPLS/protein_stepwise/mousecys/1-prep/params/selenolate-proteinprep_1-impref.maegz"
  build types name complex
QUIT

SETMODEL
  setpotential
    mmechanics  notors all
    weight constraints buffer 10
  quit
  read parm file "paramstd.dat" noprint
  energy parm dielectric 4 dist -
   listupdate 10 -
   cutoff 10 residue
  zonecons genbuffer name complex allheavy
QUIT

MINIMIZE
  write restart coordinates unformatted real8 file "selenolate-proteinprep_1-impref-out.init_coord.rst"
  input cntl mxcyc 100 rmscut 5.00000e-02 deltae 1.00000e-05 
  conjugate dx0 5.00000e-02 dxm 1.00000e+00
  run 
  write restart coordinates unformatted real8 file "selenolate-proteinprep_1-impref-out.min0_coord.rst"
  read restart coordinates unformatted real8 file "selenolate-proteinprep_1-impref-out.init_coord.rst"
QUIT

SETMODEL
  setpotential
    mmechanics
    weight constraints buffer 10
  quit
  read parm file "paramstd.dat" noprint
  energy parm dielectric 4 dist -
   cutoff 10 residue 
  zonecons genbuffer name complex allheavy
QUIT

MINIMIZE
  read restart coordinates unformatted real8 file "selenolate-proteinprep_1-impref-out.min0_coord.rst"
  input cntl mxcyc 100 rmscut 5.000000e-02 deltae 1.000000e-05
  conjugate dx0 5.000000e-02 dxm 1.000000e+00
  run
  write maestro file "selenolate-proteinprep_1-impref-out_ref0.maegz"
QUIT

END
